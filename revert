base_dir=$(dirname "$0")

# Use the batch file as an entry point if on cygwin.
if [ "${OSTYPE}" = "cygwin" -a "${TERM}" = "cygwin" ]; then
   ${base_dir}/revert.bat "$@"
   exit
fi

exec python "$base_dir/revert.py" "$@"
